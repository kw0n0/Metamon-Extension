{
  "manifest_version": 3,
  "name": "Metamon Extension",
  "version": "1.0.0",
  "description": "I have nothing",
  "icons": {
    "16": "images/metamon.png",
    "48": "images/metamon.png"
  },
  "action": {
    "default_popup": "popup/popup.html",
    "default_icon": "images/metamon.png"
  },
  "background": {
    "service_worker": "background.js",
    "type": "module"
  },
  "content_scripts": [
    {
      "js": ["scripts/index.js"],
      "matches": ["<all urls>"]
    }
  ],
  "commands": {
    "_execute_action": {
      "suggested_key": {
        "default": "Ctrl+B",
        "mac": "Command+B"
      }
    }
  }
}
